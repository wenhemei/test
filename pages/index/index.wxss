/**index.wxss**/
.container{
  padding: 20rpx;
  background: #fff;
  overflow: scroll;
}
.move_box{
  width: 100%;
  height: 100%;
  background: #fff;
}
.move_item{
  z-index: 9999;
}
.move_img{
  width: 150rpx;
  height: 150rpx;
  border-radius: 50%;
}
.swiper_item{
  width: 100%;
}
.imgs_box{
  margin-bottom: 20rpx;
}
.title_top{
  display: flex;
  justify-content: space-between;
  align-items: center;
  width: 100%;
  padding: 40rpx 40rpx 0 40rpx;
  box-sizing: border-box;
}
.title{
  font-size: 40rpx;
  width: 30%;
}
.title_top:first-child{
  border: 0;
}
.img_row{
  padding: 10rpx;
}
.load_more{
  font-size: 28rpx;
  width: 100%;
  display: flex;
  justify-content: flex-end;
  color: rgb(185, 38, 166);
}
.clearfix::after{
  content: "";
  display: table;
  clear: both;
}
.img_col {
  float: left;
  margin: 5px;
  width: 150rpx;
  height: 229.2rpx;
  box-shadow: 1px 2px 2px rgba(0, 0, 0, 0.2); 
}